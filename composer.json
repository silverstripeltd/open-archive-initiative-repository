{
    "name": "silverstripe-terraformers/open-archive-initiative-repository",
    "description": "Controller and Services that allow you to serve data as an Open Archive Initiative Repository",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "open archive iniative",
        "oai",
        "api"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "php": ">=8.1",
        "silverstripe/framework": "^5",
        "symbiote/silverstripe-queuedjobs": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "slevomat/coding-standard": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Terraformers\\OpenArchive\\": "src/",
            "Terraformers\\OpenArchive\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "silverstripe/vendor-plugin": true,
            "silverstripe/recipe-plugin": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
